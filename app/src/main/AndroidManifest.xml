<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.clearsights.mymiwokstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.github.clearsights.mymiwokstudy.PhraseActivity"
            android:parentActivityName="com.github.clearsights.mymiwokstudy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.github.clearsights.mymiwokstudy.MainActivity"/>
        </activity>
        <activity
            android:name="com.github.clearsights.mymiwokstudy.ColorsActivity"
            android:parentActivityName="com.github.clearsights.mymiwokstudy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.github.clearsights.mymiwokstudy.MainActivity"/>
        </activity>
        <activity
            android:name="com.github.clearsights.mymiwokstudy.FamilyActivity"
            android:parentActivityName="com.github.clearsights.mymiwokstudy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.github.clearsights.mymiwokstudy.MainActivity"/>
        </activity>
        <activity
            android:name="com.github.clearsights.mymiwokstudy.NumbersActivity"
            android:label="@string/title_activity_numbers"
            android:parentActivityName="com.github.clearsights.mymiwokstudy.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.github.clearsights.mymiwokstudy.MainActivity"/>
        </activity>
        <activity android:name="com.github.clearsights.mymiwokstudy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>